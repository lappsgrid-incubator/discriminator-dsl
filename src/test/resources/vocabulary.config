	uri 'http://vocab.lappsgrid.org/Document'
	description "Any electronic media content (other than computer programs or system files), including text (language), audio recordings, images, videos, web pages, etc. A document may consist of several physical computer files."
}
*/
"text-document" {
	uri 'http://vocab.lappsgrid.org/TextDocument'
	description "Any electronic media content consisting of text (language). A text document may consist of several physical computer files."
}
"audio-document" {
	uri 'http://vocab.lappsgrid.org/AudioDocument'
	description "Any electronic media content consisting of audio (language). An audio document may consist of several physical computer files."
}
